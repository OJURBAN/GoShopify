                }
                
                if(gkt.state && gkt.state.activeCampaignName == "EMPTY"){
                    gkt.utility.getUrlRewardCampaignName()
                }


                setInterval(async () => {
                    if (!isIntervalActive) {
                        // console.log("globalListener-active")

